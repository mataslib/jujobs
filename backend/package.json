{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "debugtsctrace": "tsc --noEmit --generateTrace tsctracedir; npx @typescript/analyze-trace tsctracedir",
    "devserver": "npx nodemon --delay 5 --watch ./dist -x \"node --inspect=9898 ./dist/aws/lambdas/devserver.js\"",
    "devserver2": "npx nodemon --delay 5 --watch ./dist -x \"DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config --inspect=9898 ./dist/express/app.js\"",
    "jestEnvTests": "DOTENV_CONFIG_PATH=.tests.env node --require dotenv/config node_modules/.bin/jest --selectProjects envTests --testNamePattern '^((?!@prodOnly).)*$' --coverage",
    "jestEnvProd": "DOTENV_CONFIG_PATH=.prod.env node --require dotenv/config node_modules/.bin/jest --selectProjects envProd --testNamePattern '@prodOnly' --coverage",
    "updatelatest": "npx npm-check-updates -u && npm install",
    "build": "tsc",
    "buildwatch": "tsc --watch",
    "createDbIndexesTests": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/createDbIndexes",
    "createDbIndexesProd": "DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config dist/cli/createDbIndexes",
    "createUniversityAdminTests": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/createUniversityAdmin",
    "createUniversityAdvertiserTests": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/createUniversityAdvertiser",
    "createUniversityAdminProd": "DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config dist/cli/createUniversityAdmin",
    "createUniversityAdvertiserProd": "DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config dist/cli/createUniversityAdvertiser",
    "createFixturesTests": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/createFixtures",
    "createFixturesProd": "DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config dist/cli/createFixtures",
    "deleteFixturesTests": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/deleteFixtures",
    "deleteFixturesProd": "DOTENV_CONFIG_PATH=.prod.env node -r dotenv/config dist/cli/deleteFixtures",
    "getStagUserToken": "node dist/cli/getStagUserToken",
    "testMongoNullVsUndefined": "DOTENV_CONFIG_PATH=.tests.env node -r dotenv/config dist/cli/testMongoNullVsUndefined"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "@swc/jest": "^0.2.24",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "*",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "gts": "^3.1.1",
    "jest": "29.4.2",
    "type-fest": "3.5.6"
  },
  "dependencies": {
    "axios": "1.3.2",
    "commander": "^10.0.0",
    "date-fns": "2.29.3",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "jose": "4.11.4",
    "mongodb": "^5.0.1",
    "mongoose": "^7.0.1",
    "nodemailer": "^6.9.1",
    "playwright": "1.30.0",
    "query-string": "7.1.3",
    "shared": "file:../shared",
    "source-map-support": "^0.5.21",
    "uuid": "9.0.0",
    "zod": "^3.21.2"
  }
}
